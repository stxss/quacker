<%= turbo_stream.replace_all("quoted_post_#{post.id}", partial: "posts/deleted_post", locals: {view: :timeline, post: post}) %>
<%= turbo_stream.remove_all(".reposts repost_#{post.id}") %>
<%= turbo_stream.remove("post_#{post.id}") %>

<% repost_ids.each do |repost_id| %>
 <%= turbo_stream.remove("post_#{repost_id}") %>
<% end %>
