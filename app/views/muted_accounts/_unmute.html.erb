<%= turbo_frame_tag "unmute-#{user.id}" do %>
  <%= button_to "Unmute @#{user.username}", muted_account_path(id: user.id), method: :delete, form: { data: {turbo_confirm: "Posts from this account will now be allowed in your Home timeline.", header: "Unmute @#{user.username}?", confirm: "Unmute", cancel: "Cancel" }}, remote: true %>
<% end %>
