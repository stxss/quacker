<%= turbo_stream_from "comments" %>
<%= turbo_stream_from "reposts" %>
<%= turbo_stream_from "likes" %>

<%= render "users/page_links" %>

<% likes.each do |like| %>
  <%= render "posts/single_post", post: like.post, original: (like.post.original if like.post.repost? || like.post.quote_post?), view: :likes %>
<% end %>
