<%= turbo_stream_from "comments" %>
<%= turbo_stream_from "retweets" %>
<%= turbo_stream_from "likes" %>
<%= turbo_stream_from "bookmarks" %>

<%= render "users/page_links" %>

<% likes.each do |like| %>
  <% original = like.tweet.original if like.tweet.retweet? || like.tweet.quote_tweet? %>
  <%= render "tweets/single_tweet", t: like.tweet, original: original, view: :likes %>
<% end %>
